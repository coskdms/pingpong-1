<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Tutor">

<insert id="insert">
	insert into tutor_app values(tutor_app_seq.nextval,#{title},#{career},#{exp},#{introduce},#{recomm},default)
</insert>

<insert id="insertFiles">
	insert into files values(files_seq.nextval,#{sysname}, #{oriname}, 'X','partner', 0)
</insert>
</mapper>